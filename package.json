{
  "name": "apparel-coming-soon",
  "version": "1.0.0",
  "description": "banner with form",
  "main": "index.html",
  "scripts": {
    "scss": "node-sass -o ./css scss/styles.scss",
    "serve": "browser-sync start --server --files 'css/*.css, js/*.js'",
    "watch": "onchange \"./scss/styles.scss\" -- npm run scss",
    "start": "run-p serve watch"
  },
  "keywords": [
    "forms",
    "validation"
  ],
  "author": "Konstantin Rynkov <pherapont@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2"
  }
}
